<section class="sidebar">
  <!-- Sidebar user panel -->
  <div class="user-panel">
    <div class="pull-left image">
      <img src="dist/img/user2-160x160.jpg" class="img-circle" alt="User Image">
    </div>
    <div class="pull-left info">
      <p><%= current_user.name.capitalize %></p>
      <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
    </div>
  </div>
  <!-- search form -->
  <form action="#" method="get" class="sidebar-form">
    <div class="input-group">
      <input type="text" name="q" class="form-control" placeholder="Search...">
      <span class="input-group-btn">
                <button type="submit" name="search" id="search-btn" class="btn btn-flat"><i class="fa fa-search"></i>
                </button>
              </span>
    </div>
  </form>
  <!-- /.search form -->
  <!-- sidebar menu: : style can be found in sidebar.less -->
  <ul class="sidebar-menu" data-widget="tree">
    <li class="header">teacher</li>


    <li class=" treeview"> <!-- active class used to already open on page referesh -->
      <a href="#">
        <i class="fa fa-dashboard"></i> <span>View Asign Cource</span>
        <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
      </a>
      <ul class="treeview-menu">
        <li class="active">
          <%= link_to "view courses", view_teacher_cources_path %>
          <%#= link_to "mark quiz", mark_quiz_path %>


        </li>
        <li>
          <%#=link_to "view all mcqs", view_mcqs_path %>
        </li>

      </ul>
    </li>





  </ul>
</section>