<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @teacher.name %>
</p>

<p>
  <strong>Age:</strong>
  <%= @teacher.age %>
</p>

<p>
  <strong>Phone:</strong>
  <%= @teacher.phone %>
</p>
<p>
  <strong>Email:</strong>
  <%= @teacher.email %>
</p>

<p>
  <strong>Course:</strong>
<ul>
  <% @teacher.teachercourses.each do |teachercourse| %>
    <li>
      <%=teachercourse.course.name%>
      <br>
      <%=teachercourse.course.boardgroup.board.name%>
      <br>
      <%=teachercourse.course.boardgroup.group.name%>
    </li>
  <%end%>
</ul>
</p>

<%= link_to 'Edit', edit_teacher_path(@teacher) %> |
<%= link_to 'Back', teachers_path %>
